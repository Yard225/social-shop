#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mac/Documents/DEV/NESTJS/social-shop/social-shop/node_modules/.pnpm/nx@18.3.5/node_modules/nx/bin/node_modules:/Users/mac/Documents/DEV/NESTJS/social-shop/social-shop/node_modules/.pnpm/nx@18.3.5/node_modules/nx/node_modules:/Users/mac/Documents/DEV/NESTJS/social-shop/social-shop/node_modules/.pnpm/nx@18.3.5/node_modules:/Users/mac/Documents/DEV/NESTJS/social-shop/social-shop/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mac/Documents/DEV/NESTJS/social-shop/social-shop/node_modules/.pnpm/nx@18.3.5/node_modules/nx/bin/node_modules:/Users/mac/Documents/DEV/NESTJS/social-shop/social-shop/node_modules/.pnpm/nx@18.3.5/node_modules/nx/node_modules:/Users/mac/Documents/DEV/NESTJS/social-shop/social-shop/node_modules/.pnpm/nx@18.3.5/node_modules:/Users/mac/Documents/DEV/NESTJS/social-shop/social-shop/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/nx@18.3.5/node_modules/nx/bin/nx-cloud.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/nx@18.3.5/node_modules/nx/bin/nx-cloud.js" "$@"
fi
